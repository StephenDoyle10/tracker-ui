{
  "name": "mern-stack-ui",
  "version": "1.0.0",
  "description": "mern-stack-ui",
  "main": "index.js",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "compile": "webpack --mode production",
    "#compile": "Generates JS bundles for production. Use with start.",
    "start": "node dist/server.js",
    "#start": "UI server. HMR is enabled in dev mode.",
    "lint": "eslint server src browser --ext js,jsx",
    "#lint": "Runs ESLint on all relevant files.",
    "lintfix": "eslint . --ext js,jsx --ignore-pattern public --fix",
    "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
    "#watch-server-hmr": "Reompile server HMR bundle on changes.",
    "dev-all": "rm dist/* && npm run watch-server-hmr",
    "#dev-all": "Dev mode: watch for server changes and start UI server",
    "heroku-postbuild": "npm run compile && ln -fs ../node_modules/bootstrap/dist public/bootstrap"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.4.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.21.0",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.19.4",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^16.14.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0",
    "serialize-javascript": "^1.9.1",
    "source-map-support": "^0.5.19",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
